  <script>
    // JavaScript code
    var carousel = document.querySelector('.carousel');
    var carImages = carousel.querySelectorAll('img');

    var currentIndex = 0;
    var totalImages = carImages.length;

    function moveCarousel() {
      carImages[currentIndex].classList.remove('moving-car');
      currentIndex = (currentIndex + 1) % totalImages;
      carImages[currentIndex].classList.add('moving-car');
    }

    setInterval(moveCarousel, 4000); // Change image every 2 seconds
  </script>



  
  
